{
  "manifest_version": 3,
  "name": "DataSpeak",
  "version": "1.0",
  "description": "Natural language interface for database queries",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icons/main-icon-16.png",
      "32": "icons/main-icon-32.png",
      "128": "icons/main-icon-128.png"
    }
  },
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "icons/main-icon-16.png",
    "32": "icons/main-icon-32.png",
    "128": "icons/main-icon-128.png"
  }
}
